            },
            "WEBRip-2160p": {
                "maxSize": 2000,
                "minSize": 34.5,
                "preferredSize": 1999
            },
            "WEBRip-720p": {
                "maxSize": 2000,
                "minSize": 12.5,
                "preferredSize": 1999
            }
        },
        "qualityProfile": {
            "1080p Balanced": {},
            "1080p Quality": {},
            "2160p Balanced": {},
            "2160p Quality": {}
        },
        "rootFolder": [
            "/raid/media/movies"
        ]
    }
}
